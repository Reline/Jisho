CREATE TABLE IF NOT EXISTS Info(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    value TEXT NOT NULL UNIQUE ON CONFLICT IGNORE,
    info TEXT
);

insertInfo:
INSERT INTO Info (value, info)
VALUES (?, ?);

rowid:
SELECT
CASE
WHEN (SELECT 1 FROM Info WHERE value = :info) == 1 THEN
    (SELECT id FROM Info WHERE value = :info)
ELSE
    (SELECT last_insert_rowid())
END;