CREATE TABLE IF NOT EXISTS Priority(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    value TEXT NOT NULL UNIQUE ON CONFLICT IGNORE,
    is_common INTEGER AS Boolean NOT NULL
);

insertPriority:
INSERT INTO Priority (value, is_common)
VALUES (?, ?);

rowid:
SELECT
CASE
WHEN (SELECT 1 FROM Priority WHERE value = :priority) == 1 THEN
    (SELECT id FROM Priority WHERE value = :priority)
ELSE
    (SELECT last_insert_rowid())
END;